#include "Resources.cpp"

class Pages : public Resource {
  public:
    using Resource::Resource;

  private:
    virtual void display(std::string &response) {
      response = "mostrar3!";
    }
    virtual void create(std::string &response) {
      response = "crear!";
    }
    virtual void update(std::string &response) {
      response = "actualizar!";
    }
    virtual void remove(std::string &response) {
      response = "borrar!";
    }
};
